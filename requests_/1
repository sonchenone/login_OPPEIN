        //打印事件
        function OpenPrint(ikey) {
            var myDate = new Date();
            var CurProCode = $("#HidProCode").val(); //document.getElementById("HidProCode").value;
            var checkcardcode = $("#HidCheckCardCode").val(); //document.getElementById("HidCheckCardCode").value;
            //window.open("FrmCheckedCardScorePrint.aspx?ProCode=" + CurProCode + "&CheckCardCode=" + checkcardcode + "&AutoID=" + myDate, "printer", "", "");
            window.open("FrmCheckedCardScorePrintV1.aspx?ProCode=" + CurProCode + "&CheckCardCode=" + checkcardcode + "&AutoID=" + myDate, "printer", "", "");
        }

        //<!--Start Add By lwx On 2018-10-18 增加展开、折叠子表格功能-->
        //展开或折叠所有子表格
        function AllOpenOrFoldRows() {
            var rowIds = $("#Tb_KpiInfoThree").getDataIDs();
            var type = $("#hidOpenOrFoldType").val();
            $.each(rowIds, function (index, rowId) {
                if (type == '1') {
                    $("#Tb_KpiInfoThree").expandSubGridRow(rowId);
                    $("#btnAllOpen").val("批量折叠");
                    $("#hidOpenOrFoldType").val("0");
                }
                else {
                    $("#Tb_KpiInfoThree").collapseSubGridRow(rowId);
                    $("#btnAllOpen").val("批量展开");
                    $("#hidOpenOrFoldType").val("1");
                }
            });
        }
        //<!--End Add By lwx On 2018-10-18 增加展开、折叠子表格功能-->


