        //Add By jyt 2020-04-30 目标值调整为支持图片的HTML内容
        $(function () {
            var aaa = $("#navDiv0 img").each(function (i) {
                if ($("#navDiv0 img").eq(i).width() > 200 || $("#navDiv0 img").eq(i).width() == 0) {
                    $("#navDiv0 img").eq(i).css("width", "200px");
                    var CurObj = $("#navDiv0 img").eq(i);
                    $("#navDiv0 img").eq(i).attr("onclick", "ShowPicMenu('Div_ShowPic',event,this);");
                }
            });
        });
        function ImageSuofang(Obj) {
            var CurMage = Obj.cloneNode();
            var img = new Image();
            img.src = CurMage.src;
            var imgContainer = window.parent.document.getElementById("Div_ShowPic");
            imgContainer.appendChild(img);
        }
        function ShowPicMenu(DivID, evt, Obj) {
            var CurMage = Obj.cloneNode();
            var img = new Image();
            img.src = CurMage.src;
            var imgContainer = window.parent.document.getElementById("Div_Img_OrgPics"); //"Div_Img_OrgPics"
            $('#Div_Img_OrgPics', parent.document).html("");
            imgContainer.appendChild(img);

            window.parent.document.getElementById(DivID).className = "cMenu";
            var WH = window.parent.document.documentElement.clientHeight;
            var CW = window.parent.document.documentElement.clientWidth;


            var iTop = (WH - 380) / 2; //获得窗口的垂直位置;
            var iLeft = (CW - 800) / 2; //获得窗口的水平位置;

            $("#" + DivID, parent.document).css({ "position": "fixed", "top": iTop, "left": iLeft });
            $("#" + DivID, parent.document).show();
            var obj = window.parent.document.getElementById(DivID);
            obj.style.display = "block";
            obj.style.visibility = "visible";
            return false;
        }
         
        //打印事件
        function CreateOneFormPage(divname) {
            var LODOP = getLodop();
            var name = divname;
            var topHtml = "<html><link href=../../../2016Css/process.css rel=stylesheet type=text/css /><body style=\"margin-top: 0px;padding: 40 0 0 10; width:730px;\">";
            var bottomHtml = "</body></html>";
            LODOP.SET_PRINT_PAGESIZE(2, 0, 0, "A4");
            LODOP.ADD_PRINT_HTM(0, 0, 900, 1060, topHtml + document.getElementById(name).innerHTML + bottomHtml);

            LODOP.PRINT_DESIGN();
        }

        function AdjustDivGridWH() {
            var wh = document.documentElement.clientHeight;
            var CW = document.documentElement.clientWidth;
            var eh = 90;
            ch = (wh - eh) + "px";
        }

        function ShowSplit(val) {
            var code = 'KHK2020010200051';
            if (val == 'card') {
                window.open('CheckCardView.aspx?CardCode=' + code);
            } else if (val == 'report') {
                window.open('ReportingViewV1.aspx?CardCode=' + code + '&AutoID=Check');
            } else if (val == 'ppt') {
                window.open('FrmCheckCardPPTView.aspx?CheckCardCode=' + code);
            }
        }
